<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:AutomaticWebInfoGetterWpfLib.Resources"
                    xmlns:conv="clr-namespace:AutomaticWebInfoGetterWpfLib.Converters"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:xwt="clr-namespace:Xceed.Wpf.Toolkit;assembly=Xceed.Wpf.Toolkit">







    <SolidColorBrush x:Key="WindowBackground" Color="#468966"/>
    <SolidColorBrush x:Key="LabelBackground" Color="#FFF0A5"/>
    <!--<Color x:Key="InterUpDownBackground">#FFB03B</Color>-->
    <SolidColorBrush x:Key="ListBoxBackground" Color="#B64926"/>
    <SolidColorBrush x:Key="SlidePartBackground" Color="#8E2800"/>

    <Style TargetType="Label">
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Background" Value="{StaticResource LabelBackground}"/>
    </Style>

   
    <Style TargetType="ListBox">
        <Setter Property="Background" Value="{StaticResource ListBoxBackground}"/>
    </Style>
    

</ResourceDictionary>